#include <stdio.h>
#include <conio.h>
#include <math.h>
void main()
{
    int a;
    printf("nhap gia tri cua a\n");
    scanf("%d",&a);
    if (sqrt(a)*sqrt(a)==a)
        printf (" a= %d la mot so chinh phuong ", a);
    else
        printf ("a=%d khong phai la mot so chinh phuong",a);
    getch ();
}
