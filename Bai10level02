#include <stdio.h>
#include <conio.h>
#include <math.h>
void main()
{
     int x1,x2,y1,y2;
     float ab,ac,cosx;
     printf("TINH GOC GIUA HAI DUONG THANG\n");
     printf("nhap x1,x2,y1,y2\n");
     scanf("%d%d%d%d",&x1,&x2,&y1,&y2);
     ab=sqrt(pow(x1,2)+pow(y1,2));
     ac=sqrt(pow(x2,2)+pow(y2,2));
     if (cosx!=0)
     {
        cosx=abs((x1*x2+y1*y2)/(ab*ac));
        printf("goc bac:=%f",cosx);
     }
     else
        printf("goc bac:=90");

     getch();


}
