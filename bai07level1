#include <stdio.h>
#include <conio.h>
void main()
{
    int ngay, thang, nam;
    printf("nhap ngay\n");
    scanf ("%d",&ngay);
    printf ("nhap thang\n");
    scanf ("%d",&thang);
    printf ("nhap nam\n");
    scanf ("%d",&nam);
    printf ("hom nay la ngay %d thang %d nam %d ",ngay, thang, nam);
    getch();

}




